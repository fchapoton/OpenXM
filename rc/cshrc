# $OpenXM: OpenXM/rc/cshrc,v 1.5 1999/12/04 02:20:08 takayama Exp $
setenv OpenXM_HOME $HOME/OpenXM
set path=($OpenXM_HOME/bin $path)
rehash
setenv ASIR_LIBDIR $OpenXM_HOME/lib/asir
setenv ASIRLOADPATH ASIRLOADPATH=$OpenXM_HOME/src/asir-contrib/packages/src:$OpenXM_HOME/src/asir-contrib/packages/sample:$OpenXM_HOME/lib/asir-contrib:.
setenv LOAD_SM1_PATH $OpenXM_HOME/lib/sm1
setenv LOAD_K_PATH   $OpenXM_HOME/lib/k097

