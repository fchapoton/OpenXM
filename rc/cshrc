# $OpenXM: OpenXM/rc/cshrc,v 1.9 2000/01/19 02:08:48 noro Exp $
setenv OpenXM_HOME $HOME/OpenXM
set path=($OpenXM_HOME/bin $path)
rehash
setenv ASIR_LIBDIR $OpenXM_HOME/lib/asir
setenv ASIRLOADPATH $OpenXM_HOME/src/asir-contrib/packages/src:$OpenXM_HOME/src/asir-contrib/packages/sample:$OpenXM_HOME/lib/asir-contrib:.
setenv ASIR_CONFIG $OpenXM_HOME/rc/asirrc
setenv LOAD_SM1_PATH $OpenXM_HOME/lib/sm1
setenv LOAD_K_PATH   $OpenXM_HOME/lib/k097
setenv LD_LIBRARY_PATH $LD_LIBRARY_PATH:/usr/lib:$OpenXM_HOME/lib


