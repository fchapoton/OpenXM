# $OpenXM: OpenXM/rc/cshrc,v 1.3 1999/11/12 02:23:42 takayama Exp $
setenv OpenXM_HOME $HOME/OpenXM
set path=($OpenXM_HOME/bin $path)
rehash
setenv ASIR_LIBDIR $OpenXM_HOME/lib/asir
setenv ASIRLOADPATH .:$OpenXM_HOME/src/asir99/lib/contrib/packages/src:$OpenXM_HOME/src/asir99/lib/contrib/packages/sample
setenv LOAD_SM1_PATH $OpenXM_HOME/lib/sm1
setenv LOAD_K_PATH   $OpenXM_HOME/lib/k097

