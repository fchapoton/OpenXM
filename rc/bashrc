# $OpenXM: OpenXM/rc/bashrc,v 1.4 1999/11/13 01:53:22 takayama Exp $
OpenXM_HOME=$HOME/OpenXM
export OpenXM_HOME
PATH=$OpenXM_HOME/bin:$PATH
ASIR_LIBDIR=$OpenXM_HOME/lib/asir
export ASIR_LIBDIR
ASIRLOADPATH=$OpenXM_HOME/src/asir-contrib/packages/src:$OpenXM_HOME/src/asir-contrib/packages/sample:.
export ASIRLOADPATH
LOAD_SM1_PATH=$OpenXM_HOME/lib/sm1
export LOAD_SM1_PATH
LOAD_K_PATH=$OpenXM_HOME/lib/k097
export LOAD_K_PATH

