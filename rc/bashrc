# $OpenXM: OpenXM/rc/bashrc,v 1.3 1999/11/12 02:23:42 takayama Exp $
OpenXM_HOME=$HOME/OpenXM
export OpenXM_HOME
PATH=$OpenXM_HOME/bin:$PATH
ASIR_LIBDIR=$OpenXM_HOME/lib/asir
export ASIR_LIBDIR
ASIRLOADPATH=.:$OpenXM_HOME/src/asir99/lib/contrib/packages/src:$OpenXM_HOME/src/asir99/lib/contrib/packages/sample
export ASIRLOADPATH
LOAD_SM1_PATH=$OpenXM_HOME/lib/sm1
export LOAD_SM1_PATH
LOAD_K_PATH=$OpenXM_HOME/lib/k097
export LOAD_K_PATH

