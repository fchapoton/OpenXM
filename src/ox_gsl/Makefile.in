prefix = @prefix@
CFLAGS = @CFLAGS@
LIBS = @LIBS@
LN_S = @LN_S@
TARGET = @TARGET@
CC = @CC@

CALL_GSL=call_gsl.o call_gsl_ode.o

all: ${TARGET}

ox_gsl: ox_gsl.o ${CALL_GSL} ox_eval.o mysig.o
	${CC} ${CFLAGS} -o ox_gsl ox_gsl.o ${CALL_GSL} ox_eval.o mysig.o ${LIBS}

.c.o: ox_gsl.h mysig.h
	${CC} ${CFLAGS} -c -g $<

clean:
	rm -f *.o *~ ox_gsl  
distclean: clean
	rm -f .configure_done

