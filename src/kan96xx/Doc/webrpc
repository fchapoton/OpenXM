#!/bin/sh
# DO NOT EDIT THIS FILE
# $OpenXM$
OpenXM_HOME=/home/nobuki/OpenXM
export OpenXM_HOME
PATH=$OpenXM_HOME/bin:$PATH
ASIR_LIBDIR=$OpenXM_HOME/lib/asir
export ASIR_LIBDIR
ASIRLOADPATH=$OpenXM_HOME/src/asir-contrib/packages/src:$OpenXM_HOME/src/asir-contrib/packages/sample:$OpenXM_HOME/lib/asir-contrib:.
export ASIRLOADPATH
ASIR_CONFIG=$OpenXM_HOME/rc/asirrc
export ASIR_CONFIG
ASIR_RSH='ssh -f -X -A '
export ASIR_RSH
LOAD_SM1_PATH=$OpenXM_HOME/lib/sm1
export LOAD_SM1_PATH
LOAD_K_PATH=$OpenXM_HOME/lib/k097
export LOAD_K_PATH
if test $LD_LIBRARY_PATH ; then
  LD_LIBRARY_PATH=$LD_LIBRARY_PATH:$OpenXM_HOME/lib  ;
else
  LD_LIBRARY_PATH=$OpenXM_HOME/lib ;
fi
export LD_LIBRARY_PATH


export OpenXM_PSTOIMG_TYPE=png
exec $OpenXM_HOME/bin/sm1 -s "[(parse) (httpd-rpc.sm1) pushfile] extension ; webrpc " 
