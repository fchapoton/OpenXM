/* Degenerate case A=C */
def fdah_aeqc(A,B,C,Y) {
   if  (A!=C)  error("fdah_aeqc: A!=C");
   N=length(B); 
   X=newvect(N);
   for (I=0; I<N; I++) {
     X[I] = Y[0][I+1]*Y[1][0]/(Y[0][0]*Y[1][I+1]);
   }
   U = (Y[0][0]^(-A))*(Y[1][0]^(C-1));
   for (I=0; I<N; I++) {
     U = U*(Y[1][I+1])^(-B[I]);
   }

   FD=1;
   for (I=0; I<N; I++) {
      FD=FD*(1-X[I])^(-B[I]);
   }
   Val = red(U*FD);
   return Val;
}
def feval(F) { 
  if (type(F) >= 4) return map(feval,F);
  return eval(F*exp(0)); 
}
