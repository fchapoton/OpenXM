/* intersection matrix C in page 17. */
def intmat() {
  D = newvect(MM+1);
  D[0] = 1/AA;
  for (I=1; I<=MM; I++) D[I] = -1/BB[I];
  C = matrix_diagonal_matrix(D);
  for (I=0; I<=MM; I++) {
    for (J=0; J<=MM; J++) {
      C[I][J] = C[I][J] + 1/(CC-AA);
    }
  }
  return C;
}

def check1() {
  C = intmat();
  D = red(matrix_det(C));
  printf("%a / ",fctr(nm(D)));
  printf("%a\n",fctr(dn(D)));
}

def ee(I) {
  E=newmat(1,MM+1);
  E[0][I] = 1;
  return E;
}

def vv(I,J) {
  if ((I==0) && (J < MM+1)) return vv0(J);
  else if (J < MM+1) return ee(I)-ee(J);
  /* J == MM+1 */
  return  ee(I);
}

def vv0(J) {
  V=ee(J)+(Alpha[MM+2]/Alpha[0])*ee(0);
  for (K=1; K<=MM; K++) {
    V += ee(K)*(Alpha[K]/Alpha[0]);
  }
  return V;
}

def dxx(I) {
  if (I == 0) return 0;
  if (I == (MM+1)) return 0;
  return util_v(dx,[I]);
