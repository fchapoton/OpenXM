   printf("Alpha_ap(Alpha(a+1)=%a,Alpha_am(Alpha(a-1))=%a\n",Alpha_ap,Alpha_am);
   printf("MyDebug=%a\n",MyDebug);
   return [MM,AA,BB,CC,Alpha,Alpha_ap,Alpha_am,MyDebug];
}
