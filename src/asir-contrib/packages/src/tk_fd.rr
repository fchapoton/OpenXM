 */
def ygDk(A,B,C,X,K) {
  Qk = ygQmat2(A+1,B,C+1,X,K); 
  Q0 = ygQmat2(A+1,B,C+1,X,0);
  return map(red,Qk*matrix_inverse(Q0));
