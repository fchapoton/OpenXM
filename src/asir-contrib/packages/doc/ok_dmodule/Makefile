#$OpenXM$

# Name of this document.
MYNAME=ok_dmodule

SRC = ok_dmodule.oxw

ENSRCS = ${SRC:.oxw=.en} 
JASRCS = ${SRC:.oxw=.ja} 
ENAUTO=  
JAAUTO=${ENAUTO:.en=.ja}
GENTEXI_OPT=--noSorting --author "OpenXM.org" --infoName asir-contrib-${MYNAME}
include ../Makefile.common

clean_private: 
	${RM} -f ${MYNAME}-ja.texi ${MYNAME}-en.texi

${MYNAME}-ja.texi: ${MYNAME}.oxg
	@echo "Ok_dmodule (Okutani =?ISO-2022-JP?B?GyRCSEcbKEI=?=  D-module =?ISO-2022-JP?B?GyRCNFgbKEI=?= =?ISO-2022-JP?B?GyRCP3QbKEI=?= ) =?ISO-2022-JP?B?GyRCJV4lSyVlGyhC?= =?ISO-2022-JP?B?GyRCJSIlaxsoQg==?=" >title.tmp
# generated by nkf -M
	${NKF} -j ${MYNAME}.oxg | ${OXWEAVE} usage-ja | ${OXGENTEXI} ${GENTEXI_OPT} --title "`nkf -e title.tmp`" >${MYNAME}-ja.texi
	rm -f title.tmp

${MYNAME}-en.texi: ${MYNAME}.oxg
	${NKF} -j ${MYNAME}.oxg | ${OXWEAVE} usage-en | ${OXGENTEXI} ${GENTEXI_OPT} --title "Ok_dmodule (a library for D-modules by Okutani) Manual" >${MYNAME}-en.texi
