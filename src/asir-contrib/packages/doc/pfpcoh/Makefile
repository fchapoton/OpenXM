#$OpenXM: OpenXM/src/asir-contrib/packages/doc/pfpcoh/Makefile,v 1.2 2005/06/15 02:53:41 takayama Exp $

# Name of this document.
MYNAME=pfpcoh

SRC = pfpcoh.oxw

ENSRCS = ${SRC:.oxw=.en} 
JASRCS = ${SRC:.oxw=.ja} 
ENAUTO= pfpcoh-auto.en
JAAUTO=${ENAUTO:.en=.ja}
GENTEXI_OPT=--noSorting --author "OpenXM.org" --infoName asir-contrib-${MYNAME}
include ../M/Makefile

clean_private: 
	${RM} -f ${MYNAME}-ja.texi ${MYNAME}-en.texi

${MYNAME}-ja.texi: ${MYNAME}.oxg
	@echo "pfpcoh (cohomology/homology groups for p F q) =?ISO-2022-JP?B?GyRCJV4lSyVlJSIlaxsoQg==?= " >title.tmp
# generated by nkf -M
	${NKF} -j ${MYNAME}.oxg | ${OXWEAVE} usage-ja | ${OXGENTEXI} ${GENTEXI_OPT} --title "`nkf -e title.tmp`" >${MYNAME}-ja.texi
	rm -f title.tmp

${MYNAME}-en.texi: ${MYNAME}.oxg
	${NKF} -j ${MYNAME}.oxg | ${OXWEAVE} usage-en | ${OXGENTEXI} ${GENTEXI_OPT} --title "Pfpcoh (cohomology/homology groups for p F q)  Manual" >${MYNAME}-en.texi

${MYNAME}-auto.en: ${CONTRIB}/pfpcoh.rr ${CONTRIB}/pfphom.rr
	${RM} -f pfp-auto.en
	cat ${CONTRIB}/pfpcoh.rr ${CONTRIB}/pfphom.rr | ${OXWEAVE} usage  | ${OXGENTEXI} --en --upnode "Pfpcohom" ${OPTGENTEXI} >${MYNAME}-auto.en

${MYNAME}-auto.ja: ${CONTRIB}/pfpcoh.rr ${CONTRIB}/pfphom.rr
	${RM} -f pfp-auto.ja
	cat ${CONTRIB}/pfpcoh.rr ${CONTRIB}/pfphom.rr | ${OXWEAVE} usage  | ${OXGENTEXI} --en --upnode "Pfpcohom" ${OPTGENTEXI} >${MYNAME}-auto.ja
