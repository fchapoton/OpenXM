#$OpenXM: OpenXM/src/asir-contrib/packages/doc/ok_diff/Makefile,v 1.1 2005/04/11 12:04:37 takayama Exp $

# Name of this document.
MYNAME=ok_diff

SRC = ok_diff.oxw

ENSRCS = ${SRC:.oxw=.en} 
JASRCS = ${SRC:.oxw=.ja} 
ENAUTO=  
JAAUTO=${ENAUTO:.en=.ja}
GENTEXI_OPT=--noSorting --author "OpenXM.org" --infoName asir-contrib-${MYNAME}
include ../M/Makefile

clean_private: 
	${RM} -f ${MYNAME}-ja.texi ${MYNAME}-en.texi

${MYNAME}-ja.texi: ${MYNAME}.oxg
	@echo "Ok_diff (Okutani =?ISO-2022-JP?B?GyRCSEcbKEI=?=  =?ISO-2022-JP?B?GyRCSHkbKEI=?= =?ISO-2022-JP?B?GyRCSiw6bk1RQUc0WD90GyhC?= ) =?ISO-2022-JP?B?GyRCJV4bKEI=?= =?ISO-2022-JP?B?GyRCJUslZSUiJWsbKEI=?= " >title.tmp
# generated by nkf -M
	${NKF} -j ${MYNAME}.oxg | ${OXWEAVE} usage-ja | ${OXGENTEXI} ${GENTEXI_OPT} --title "`nkf -e title.tmp`" >${MYNAME}-ja.texi
	rm -f title.tmp

${MYNAME}-en.texi: ${MYNAME}.oxg
	${NKF} -j ${MYNAME}.oxg | ${OXWEAVE} usage-en | ${OXGENTEXI} ${GENTEXI_OPT} --title "Ok_diff (a library for differential operators by Okutani) Manual" >${MYNAME}-en.texi
