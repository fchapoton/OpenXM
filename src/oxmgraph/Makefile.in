OpenXM_HOME = @prefix@
CFLAGS = @CFLAGS@
CFLAG2 = @CFLAG2@
LFLAG2 = @LFLAG2@
MYTARGET = @MYTARGET@
MYTARGET2 = @MYTARGET2@
CC = @CC@
all: ${MYTARGET}

${MYTARGET}: graph.c
	${CC} ${CFLAGS} ${CFLAG2} graph.c ${LFLAG2} -o ${MYTARGET}

install: ${MYTARGET2}
install2: ${MYTARGET}
	install  ${MYTARGET} ${OpenXM_HOME}/bin

install-for-debug:
	ln -s `pwd`/graph  ${OpenXM_HOME}/bin/oxmgraph

clean:
	rm -rf ${MYTARGET} *.o *~ config.log config.status
distclean: clean
	rm -f Makefile

