prefix = @prefix@
CFLAGS = @CFLAGS@
LIBS = @LIBS@
LN_S = @LN_S@
TARGET = @TARGET@
CC = @CC@

all: ${TARGET}

ox_python: ox_python.o mysig.o
	${CC} ${CFLAGS} `python2-config --cflags` -o ox_python ox_python.o mysig.o `python2-config --ldflags` ${LIBS}

.c.o: ox_python.h mysig.h
	${CC} ${CFLAGS} `python2-config --cflags` -c -g $<

clean:
	rm -f *.o *~ ox_python  
distclean: clean
	rm -f .configure_done

