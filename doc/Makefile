#$OpenXM$
OXWEAVE=../src/kxx/oxweave
OW_FLAG=--recursive
all : $(OXWEAVE) install-jp.txt  install-eg.txt

install-jp.txt : install.oxweave
	-rm install-jp.txt
	echo "DO NOT EDIT THIS FILE." >install-jp.txt
	$(OXWEAVE) $(OW_FLAG) jp C <install.oxweave >>install-jp.txt

install-eg.txt : install.oxweave
	-rm install-eg.txt
	echo "DO NOT EDIT THIS FILE." >install-eg.txt
	$(OXWEAVE) $(OW_FLAG) eg C <install.oxweave >>install-eg.txt

$(OXWEAVE) : $(OXWEAVE).c
	(cd ../src/kxx ; make oxweave)
